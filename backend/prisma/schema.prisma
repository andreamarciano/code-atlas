generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Language {
  id   Int    @id @default(autoincrement())
  name String @unique
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}
